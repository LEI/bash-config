--prefix src/git --name .gitconfig

--prefix src/tmux \
--name .tmux.conf \
--create-dir .tmux \
--path .tmux/tmux.*.conf \
--path .tmux/tmux-$UNAME.conf \
--path d:.tmux/*

--prefix src/vim \
--create-dir .vim \
--create-dir .vim/{plugin,settings} \
--path .vim/plugin/*.vim \
--path .vim/settings/*.vim \
--path f:.vim/*

