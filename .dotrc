# --prefix src/config
# --create-dir .config

--prefix src/bash \
--create-dir .bash.d \
--path .bash*

--prefix src/git --path .git*

--prefix src/tmux \
--name .tmux.conf \
--create-dir .tmux \
--path .tmux/tmux.*.conf \
--path .tmux/tmux-$UNAME.conf \
--path d:.tmux/*

--prefix src/vim \
--create-dir .vim \
--create-dir .vim/{plugin,settings} \
--path .vim/plugin/*.vim \
--path .vim/settings/*.vim \
--path f:.vim/*
